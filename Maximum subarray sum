/* The problem statement can be found at the below link.*/
/* https://cses.fi/problemset/task/1643/ */
/* For this problem we have produced two solutions the first one is a brute force which does not satisfy all the testcases because it exceeds
time limit in some testcase and the second one is an effecient one which satisfies all testcases.*/
/* Solution 1:-*/
#include<bits/stdc++.h>
#define ll long long
using namespace std;

const int MAXN = 2e5+1;
ll a[MAXN];

int main(){
	int n;
	cin >> n;
	for(int i=1;i<=n;i++){
		ll x;
		cin >> x;
		a[i] = a[i-1]+x;
	}
	ll ans=-99999999999;
	for(int i=1;i<=n;i++){
		for(int j=1;j<=i;j++){
			if(a[i]-a[j-1]>ans){
				ans = a[i]-a[j-1];
			}
		}
	}
	cout << ans;
}
/* Solution 2:-*/
#include<bits/stdc++.h>
#define ll long long 
using namespace std;

int main(){
	int n;
	cin >> n;
	vector<ll> pfr(n+1,0);
	for(int i=1;i<=n;i++){
		int x;
		cin >> x;
		pfr[i] = pfr[i-1]+x;
	}
	ll maxi = pfr[1],mini=pfr[0];
	for(int i=1;i<=n;i++){
		maxi = max(maxi,pfr[i]-mini);
		mini = min(mini,pfr[i]);
	}
	cout << maxi;
}

