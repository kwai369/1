#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main(){
	int a[10];
	for(int i=0;i<10;i++){
		cin >> a[i];
	}
	int ans=0;
	for(int i=0;i<3;i++){
		ans = a[ans];
	}
	cout << ans;
	return 0;
}

