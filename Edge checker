/* The problem statement can be found at the below link.*/
/* https://atcoder.jp/contests/abc240/tasks/abc240_a */
#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main(){
	int a[10][2];
	a[0][0] = 10,a[0][1] = 2;
	a[1][0] = 1,a[1][1] = 3;
	a[2][0] = 2,a[2][1] = 4;
	a[3][0] = 3,a[3][1] = 5;
	a[4][0] = 4,a[4][1] = 6;
	a[5][0] = 5,a[5][1] = 7;
	a[6][0] = 6,a[6][1] = 8;
	a[7][0] = 7,a[7][1] = 9;
	a[8][0] = 8,a[8][1] = 10;
	a[9][0] = 9,a[9][1] = 1;
	int x,y;
	cin >> x >> y;
	x--;
	if(a[x][0] != y && a[x][1] != y){
		cout << "No";
	} 
	else{
		cout << "Yes";
	}
	return 0;
}

