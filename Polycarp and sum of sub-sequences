/* The problem statement can be found at the below link. */
/* https://codeforces.com/contest/1618/problem/A */
#include<bits/stdc++.h>
#define ll long long
using namespace std;
 
void testcase(){
	ll arr[7];
	for(int i=0;i<7;i++){
		cin >> arr[i];
	}
	cout << arr[0] << " " << arr[1] << " " << arr[6]-(arr[0]+arr[1]);
}
 
int main(){
	int t;
	cin >> t;
	while(t){
		testcase();
		cout << "\n";
		t--;
	}
	return 0;
}
