    #include <bits/stdc++.h>
    #define ll long long 
    using namespace std;
     
    int main(){
    	int a,b,c,d;
    	cin >> a >> b >> c >> d;
    	if(a > c){
    		cout << "Aoki";
    	}
    	else if(a < c){
    		cout << "Takahashi";
    	}
    	else{
    		if(b<=d){
    			cout << "Takahashi";
    		}
    		else{
    			cout << "Aoki";
    		}
    	}
    }
